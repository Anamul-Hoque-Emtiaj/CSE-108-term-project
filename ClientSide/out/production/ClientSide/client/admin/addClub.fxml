<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="235.0" prefWidth="275.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.admin.AddClubController">
   <children>
      <Text layoutX="70.0" layoutY="55.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Add Club">
         <font>
            <Font name="Verdana Bold" size="25.0" />
         </font>
      </Text>
      <Label layoutX="35.0" layoutY="85.0" text="Club Name:">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <TextField fx:id="name" layoutX="35.0" layoutY="120.0" prefHeight="30.0" prefWidth="184.0" promptText="Enter Club's Name" />
      <Button layoutX="35.0" layoutY="170.0" mnemonicParsing="false" onAction="#submit" text="Submit">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="155.0" layoutY="170.0" mnemonicParsing="false" onAction="#cancel" text="Cancel">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
