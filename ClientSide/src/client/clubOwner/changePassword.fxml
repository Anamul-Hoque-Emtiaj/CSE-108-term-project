<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="303.0" prefWidth="395.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.clubOwner.ChangePasswordController">
   <children>
      <Text layoutX="60.0" layoutY="50.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Change Password">
         <font>
            <Font name="Verdana Bold" size="27.0" />
         </font>
      </Text>
      <Label layoutX="35.0" layoutY="90.0" text="Club Name:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="35.0" layoutY="130.0" text="Old Password:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="35.0" layoutY="170.0" text="New Password:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="35.0" layoutY="210.0" text="Confirm Password:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button layoutX="180.0" layoutY="250.0" mnemonicParsing="false" onAction="#submit" text="Submit">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="285.0" layoutY="250.0" mnemonicParsing="false" onAction="#cancel" text="Cancel">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <PasswordField fx:id="oldPassword" layoutX="160.0" layoutY="128.0" prefHeight="26.0" prefWidth="194.0" />
      <PasswordField fx:id="newPassword" layoutX="160.0" layoutY="167.0" prefHeight="26.0" prefWidth="194.0" />
      <PasswordField fx:id="confirmPassword" layoutX="161.0" layoutY="207.0" prefHeight="26.0" prefWidth="194.0" />
      <Text fx:id="name" layoutX="161.0" layoutY="105.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name">
         <font>
            <Font size="15.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
