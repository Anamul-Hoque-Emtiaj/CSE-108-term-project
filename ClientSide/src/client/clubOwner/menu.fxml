<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.clubOwner.MenuController">
   <children>
      <Button layoutX="40.0" layoutY="70.0" mnemonicParsing="false" onAction="#searchPlayer" prefHeight="31.0" prefWidth="138.0" text="Search Players">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button layoutX="40.0" layoutY="125.0" mnemonicParsing="false" onAction="#addPlayer" prefHeight="31.0" prefWidth="138.0" text="Add Player">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button layoutX="40.0" layoutY="180.0" mnemonicParsing="false" onAction="#buyPlayer" prefHeight="31.0" prefWidth="138.0" text="Buy Player">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button layoutX="40.0" layoutY="340.0" mnemonicParsing="false" onAction="#changePassword" text="Change Password">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button layoutX="40.0" layoutY="285.0" mnemonicParsing="false" onAction="#pendingRequest" prefHeight="31.0" prefWidth="138.0" text="Pending Request">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Text fx:id="welcome" layoutX="255.0" layoutY="55.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome ">
         <font>
            <Font size="25.0" />
         </font>
      </Text>
      <Button layoutX="490.0" layoutY="30.0" mnemonicParsing="false" onAction="#logout" text="Log out">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button layoutX="40.0" layoutY="235.0" mnemonicParsing="false" onAction="#clubDetails" prefHeight="31.0" prefWidth="138.0" text="Club Details">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
